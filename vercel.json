{
  "framework": "angular",
  "buildCommand": "ng build",
  "outputDirectory": "dist/HomeSync/browser",
  "functions": {
    "dist/HomeSync/server/server.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/render"
    }
  ],
  "headers": [
    {
      "source": "/assets/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}